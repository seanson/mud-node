{
    "plugins": ["babel"],
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["warn", 4, {"SwitchCase": 1}],
        "react/jsx-indent": ["warn", 4],
        "react/jsx-indent-props": ["warn", 4],
        "jsx-a11y/href-no-hash": 0
    }
}
